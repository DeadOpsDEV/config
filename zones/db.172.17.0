;
; BIND data file for local loopback interface
;
$TTL    604800
@       IN      SOA     server.loic-barel.local. root.loic-barel.local. (
                              2         ; Serial
                         604800         ; Refresh
                          86400         ; Retry
                        2419200         ; Expire
                         604800 )       ; Negative Cache TTL
;
@       IN      NS      server.loic-barel.local.
1       IN      PTR     server.loic-barel.local.
10      IN      PTR     smtp.loic-barel.local.
12      IN      PTR     pop.loic-barel.local.
11      IN      PTR     imap.loic-barel.local




